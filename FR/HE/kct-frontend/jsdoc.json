{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "exclude": ["node_modules"] 
  },
  "plugins": ["plugins/markdown"],
  
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "docs",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/docdash",
    "encoding": "utf8"
  }
}